<!DOCTYPE html>
<html>
  <head>
    <title>Star Puzzle</title>
    <!-- <link rel="stylesheet" type="text/css" href="/css/main.css"> -->
    <link rel="stylesheet" type="text/css" href="/build/css/main.css">
    <script>
    <% if (user) { %>
      var user = <%- JSON.stringify(user) %>;
      var duplicate = <%- JSON.stringify(duplicate) %>;
    <% } else { %>
      var user = null;
    <% } %>
    </script>
  </head>
  <body>
    <div id="game">
      <div id="blur-game" class="blur disable"></div>
    </div>
    <div id="ajax-loading" class="disable"><img src="https://i.gifer.com/DzUd.gif" /></div>
    <div id="lobby-chatting" class="disable">
      <div id="lobby-chat-container"></div>
      <div id="lobby-chat-users"><div id="lobby-chat-user-count">0</div></div>
      <input id="lobby-chat-input" type="text" maxlength="125">
      <button id="lobby-chat-submit">Enter</button>
    </div>
    <div id="game-chatting" class="disable">
      <div id="game-chat-container"></div>
      <input id="game-chat-input" type="text" maxlength="125">
      <button id="game-chat-submit">Enter</button>
    </div>
    <div id="ad-container" class="disable"></div>
    <!-- <script src="//cdnjs.cloudflare.com/ajax/libs/phaser/2.6.2/phaser.min.js"></script> -->
    <!-- <script src="//cdn.jsdelivr.net/npm/phaser-ce@2.11.0"></script> --> <!-- minified version -->
    <script src="//cdn.jsdelivr.net/npm/phaser-ce@2.11.0/build/phaser.js"></script>
    <script src="/build/bundle.min.js"></script>
    <!-- <script src="/js/util/global.js"></script> -->
    <!-- <script src="/js/util/phaser-input.js"></script> -->
    <!-- <script src="/js/bundle.js"></script> -->
  </body>
</html>
